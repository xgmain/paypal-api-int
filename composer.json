{
    "name": "xgao/paypal-api-int",
    "description": "php laravel paypal payment gateway api integration",
    "keywords": ["paypal", "payment gateway", "php", "laravel"],
    "author": {
        "name": "xin gao",
        "email": "xin.gao.au@outlook.com"
    },
    "license": "MIT",
    "require": {
		"php": ">=7.2|^8.0",
        "ext-curl": "*",
        "guzzlehttp/guzzle": "~7.0",
        "illuminate/support": "~6.0|~7.0|~8.0|~9.0|^10.0",
        "nesbot/carbon": "~2.0"
	},
	"require-dev": {
        "phpunit/phpunit": "^8.0|^9.0",
        "symfony/var-dumper": "~5.0"
    },
	"autoload": {
		"psr-4": {
			"PayPal\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"PayPal\\Tests\\": "tests"
		}
	},
	"scripts": {
		"test": "phpunit tests"
	}
}